passenger_root /usr/local/rbenv/versions/2.0.0-p451/lib/ruby/gems/2.0.0/gems/passenger-4.0.41;
passenger_ruby /usr/local/rbenv/shims/ruby;
passenger_max_pool_size 6;
passenger_spawn_method smart-lv2;
passenger_buffer_response on;
passenger_min_instances 1;
passenger_max_instances_per_app 0;
passenger_pool_idle_time 300;
passenger_max_requests 0;
passenger_app_env development;

server {
    listen 80;
    server_name localhost;
    root /var/www/html/kidsapp/current/public;
    passenger_enabled on;
}
